algoritmo "Enquanto II"

var
   i: inteiro
inicio
   i <- 0
   
   escreval("Início")
   enquanto i <= 100 faca
      escreval(i)
      i <- i + 1
      
      se i = 50 entao
         escreval("O i vale 50, senhoras e senhores!")
         interrompa
      fimse
   fimenquanto
   escreval("Fim")
fimalgoritmo
